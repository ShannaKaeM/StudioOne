    "input": {
      "name": "Input",
      "category": "form",
      "description": "Form input element",
      "variables": {
        "width": "100%",
        "padding": "0.75rem",
        "backgroundColor": "hsl(0, 0%, 5%)",
        "borderWidth": "1px",
        "borderColor": "hsl(0, 0%, 20%)",
        "borderRadius": "0.375rem",
        "color": "white"
      }
    }
