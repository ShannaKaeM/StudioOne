    "text-logo": {
      "name": "Text Logo",
      "category": "typography",
      "description": "Logo text element",
      "variables": {
        "fontSize": "calc(min(1rem, max(3rem)))",
        "fontWeight": "700",
        "color": "hsl(347, 37%, 50%)"
      }
    },
    "link": {
      "name": "Link",
      "category": "interactive",
      "description": "Interactive link element",
      "variables": {
        "color": "hsl(0, 0%, 70%)",
        "textDecoration": "none",
        "padding": "0.5rem 1rem",
        "cursor": "pointer",
        "transition": "all 0.2s ease",
        "fontSize": "1rem",
        "fontWeight": "300"
      },
      "states": {
        "hover": {
          "color": "hsl(220, 70%, 50%)",
          "fontWeight": "500"
        }
      }
    },
    "button": {
      "name": "Button",
      "category": "interactive",
      "description": "Interactive button element",
      "variables": {
        "display": "inline-flex",
        "alignItems": "center",
        "justifyContent": "center",
        "padding": "0.5rem 1rem",
        "fontSize": "0.875rem",
        "fontWeight": "500",
        "borderRadius": "0.375rem",
        "backgroundColor": "hsl(220, 70%, 50%)",
        "color": "white",
        "cursor": "pointer",
        "transition": "all 0.2s ease"
      },
      "states": {
        "hover": {
          "backgroundColor": "hsl(220, 70%, 40%)"
        }
      }
    },
    "title": {
      "name": "Title",
      "category": "typography",
      "description": "Title text element",
      "variables": {
        "fontSize": "1.5rem",
        "fontWeight": "600",
        "margin": "0 0 1rem 0"
      }
    },
    "text": {
